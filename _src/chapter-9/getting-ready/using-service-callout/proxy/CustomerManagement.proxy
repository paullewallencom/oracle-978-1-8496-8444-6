                    <con2:outboundTransform>
                      <con3:replace varName="body" contents-only="true">
                        <con1:id>_ActionId-3435026670021032855-6bf4bded.1310548a29a.-7e21</con1:id>
                        <con3:expr>
                          <con1:xqueryTransform>
