      <con4:wsdl ref="using-service-callout/wsdl/CustomerManagement"/>
      <con4:port>
        <con4:name>CustomerManagementSOAP</con4:name>
        <con4:namespace>http://www.somecorp.org/CustomerManagement</con4:namespace>
